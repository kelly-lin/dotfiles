arr=(${(f)"$(tmux list-sessions | \
  sed -E 's/:.*$//' | \
  grep -v "^$(tmux display-message -p '#S')" | \
  fzf --reverse -m
)"})
for session_name in $arr; do
  echo "session name is: $session_name"
done

  # tmux kill-session -t "$session"
